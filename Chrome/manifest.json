{
  "manifest_version": 2,
  "name": "Leads Tracker",
  "version": "1.0",
  "description": "Save and manage your leads directly from the browser.",
  "permissions": ["tabs", "storage"],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
